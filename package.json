    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@google-cloud/kms": "^5.1.0",
    "@langchain/community": "^0.3.49",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.4.5",
    "@langchain/openai": "^0.6.3",
    "@supabase/supabase-js": "^2.50.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.2",
    "d3-dsv": "2",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.0",
    "langsmith": "^0.3.72",
    "mammoth": "^1.10.0",
    "pdf-parse": "^1.1.1",
