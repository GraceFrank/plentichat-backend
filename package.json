{
  "name": "plentichat-backend",
  "version": "1.0.0",
  "description": "PlentiChat AI Agent Backend Server",
  "main": "dist/server.js",
  "scripts": {
    "dev": "docker-compose -f docker-compose.dev.yml up --build",
    "dev:local": "tsx watch --clear-screen=false src/server.ts",
    "dev:local:env": "nodemon --watch src --watch .env --exec tsx src/server.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:webhook": "tsx test-webhook.ts",
    "generate:signature": "tsx scripts/generate-signature.ts"
  },
  "keywords": [
    "fastify",
    "ai",
    "instagram",
    "webhook"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@bull-board/api": "^6.13.1",
    "@bull-board/fastify": "^6.13.1",
    "@bull-board/ui": "^6.13.1",
    "@fastify/basic-auth": "^6.2.0",
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.1.1",
    "@google-cloud/kms": "^5.1.0",
    "@langchain/community": "^1.0.0",
    "@langchain/core": "^1.0.2",
    "@langchain/langgraph": "^1.0.1",
    "@langchain/langgraph-checkpoint-redis": "^1.0.0",
    "@langchain/openai": "^1.0.0",
    "@react-email/components": "^0.5.7",
    "@react-email/render": "^1.4.0",
    "@sentry/node": "^10.23.0",
    "@sentry/profiling-node": "^10.23.0",
    "@supabase/supabase-js": "^2.78.0",
    "axios": "^1.10.0",
    "bull-board": "^2.1.3",
    "bullmq": "^5.61.0",
    "cheerio": "^1.1.2",
    "d3-dsv": "2",
    "dotenv": "^16.4.5",
    "fastify": "^5.2.0",
    "ioredis": "^5.8.1",
    "langchain": "^1.0.2",
    "langsmith": "^0.3.80",
    "luxon": "^3.7.2",
    "mammoth": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.4.0",
    "twilio": "^5.10.4",
    "word-extractor": "^1.0.4",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^22.10.2",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.10",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16"
  }
}
